{
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact",
        "postcss": "css"
    },
    "emmet.showExpandedAbbreviation": "always",
    "emmet.triggerExpansionOnTab": true,
    "files.associations": {
        "*.js": "javascriptreact",
        "*.css": "postcss"
    },
    "editor.formatOnSave": false,
    "eslint.autoFixOnSave": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "typescript",
            "autoFix": true
        },
        {
            "language": "typescriptreact",
            "autoFix": true
        }
    ]
}